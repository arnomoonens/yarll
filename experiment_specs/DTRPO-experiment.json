{
    "experiment_name": "CartPole-DPPO",
    "environments": {
        "type": "single",
        "source": "CartPole-v0"
    },
    "agent": {
        "name": "TRPO",
        "args": {
            "monitor_path": "/tmp/CartPole-v0-DPPO",
            "n_hidden_units": 10,
            "n_hidden_layers": 1,
            "gradient_clip_value": 50.0,
            "batch_size": 64,
            "learning_rate": 3e-4,
            "vf_coef": 1.0,
            "n_local_steps": 512,
            "n_workers": 4,
            "cso_epsilon": 0.2,
            "n_epochs": 10,
            "video": false,
            "save_model": true,
            "monitor": true,
	        "n_iter": 400000
        }
    }
}
